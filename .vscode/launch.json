{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug MT-Bench Large",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/eval/eval_mt_bench.py",
            "args": [
                "--eval_mode",
                "large",
                "--target_model",
                "vicuna-13b-v1.5",
                "--draft_model",
                "vicuna-68m",
                "--gamma",
                "5",
                "-n",
                "1",
                "-e",
                "vicuna",
                "--max_tokens",
                "128",
                "--temp",
                "0"
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "0"
            },
            "console": "integratedTerminal"
        }
    ]
}